import "math.vlad";

fun print_elements(arr : array, n : int) -> void {
	for(var i = 0; i < n; i++) {
		print(arr[i]);
	}
}

fun even_to(n : int) -> array {
	var arr[n] : int;

	var i : int = 0;
	while(i < n) {
		arr[i] = (i * 2);
		i++;
	}

	return arr;
}

print("--------Factorial--------");
print(factorial(5));

print("-----Array elements------");
var n : int = 10;
var even = even_to(n);
print_elements(even, n);

